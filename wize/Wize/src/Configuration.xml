<Configuration>  
	<Module>
		<Type>Weight</Type>
		<Web>http://fansrating.com/Scales/Client</Web>
		<ResetTime>15</ResetTime>
		<OfflineInterval>1</OfflineInterval>
	</Module>
	<Controller>
		<Type>Siemens</Type>
		<IP>192.168.2.126</IP> 
		<Port>2000</Port>
	</Controller>
	<LPR>
		<Type>HIKVision</Type>
		<IP>192.168.2.100</IP> 
		<Port>7600</Port>
	</LPR>
	<Tag>
		<Type>Keys</Type>
		<Com>COM5</Com>		
	</Tag>
	<Display>
		<Type>WeightDisplay</Type>
		<IsShowUserInput>true</IsShowUserInput>
		<IsDisplay>true</IsDisplay>
	</Display>
	<Scale>
		<Type>Raspberry</Type>
		<Scale>J6478</Scale>
		<Com>COM5</Com>
	</Scale>
	<IO>
		<Type>Raspberry</Type>
		<GPIO>
			<IO>IO_02</IO>
			<State>High</State>
			<Transput>Output</Transput>	 
		</GPIO>	
	</IO>
</Configuration>

